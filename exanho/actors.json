{
    "exanho_actors": [
        {
            "name": "SampleService",
            "kind": "RpcServer",
            "daemon": false,
            "services":[
                {
                    "handler_module": "exanho.services.sample",
                    "address": {
                        "host": "",
                        "port": 3120
                    },
                    "secret_key": "qwerty",
                    "concurrency": {
                        "degree": 2,
                        "kind": "thread"
                    }
                }
            ]            
        },
        {
            "name": "NsiOrgTypeService",
            "kind": "RpcServer",
            "daemon": true,
            "services":[
                {
                    "handler_module": "exanho.services.nsi.organization_type",
                    "address": {
                        "host": "",
                        "port": 3121
                    },
                    "secret_key": "password",
                    "concurrency": {
                        "degree": 2,
                        "kind": "thread"
                    },
                    "db_domain": {
                        "url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "validate": true
                    }                    
                }
            ]            
        },
        {
            "name": "RunFtpTaskWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.workers.ftp_task_run",
                    "sleep": 5.0,
                    "db_domain": {
                        "url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "validate": true
                    } 
                }
            ]
        },
        {
            "name": "LoadFtpFileWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.workers.ftp_file_load",
                    "sleep": 5.0,
                    "db_domain": {
                        "url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "validate": true
                    } 
                }
            ]
        },
        {
            "name": "ParseFtpContentWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.workers.ftp_content_parse",
                    "sleep": 5.0,
                    "db_domain": {
                        "url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "validate": true
                    } 
                }
            ]
        }
    ]
}
{
    "exanho_actors": [
        {
            "name": "SampleService",
            "kind": "RpcServer",
            "daemon": false,
            "services":[
                {
                    "handler_module": "exanho.services.sample",
                    "address": {
                        "host": "",
                        "port": 3120
                    },
                    "secret_key": "qwerty",
                    "concurrency": {
                        "degree": 2,
                        "kind": "thread"
                    }
                }
            ]            
        },
        {
            "name": "SampleWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.workers.sample",
                    "sleep": 5
                }
            ]
        }
    ]
}
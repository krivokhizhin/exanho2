{
    "exanho_context":{
        "joinable_queues": [
            {
                "name": "parse_notification",
                "maxsize": 1000
            }
        ]
    },
    "exanho_actors": [
        {
            "name": "SampleService",
            "kind": "RpcServer",
            "daemon": true,
            "services":[
                {
                    "handler_module": "exanho.sample.services.sample",
                    "address": {
                        "host": "",
                        "port": 3120
                    },
                    "secret_key": "qwerty",
                    "concurrency": {
                        "degree": 2,
                        "kind": "thread"
                    }
                }
            ]            
        },
        {
            "name": "RunFtpTaskWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.task_run",
                    "sleep": 5.0,
                    "appsettings":{
                        "db_url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44_test",
                        "db_validate": true,
                        "ftp_host": "ftp.zakupki.gov.ru",
                        "ftp_port": 21,
                        "ftp_user": "free",
                        "ftp_password": "free"
                    }
                }
            ]
        },
        {
            "name": "LoadFtpFileWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "joinable_queues": ["parse_notification"],
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.file_load",
                    "sleep": 5.0,
                    "appsettings":{
                        "db_url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44_test",
                        "db_validate": true,
                        "ftp_host": "ftp.zakupki.gov.ru",
                        "ftp_port": 21,
                        "ftp_user": "free",
                        "ftp_password": "free",
                        "parse_queues":["parse_notification"],
                        "queue_by_filter":[1],
                        "parse_filters":["notification_Adygeja_Resp"],
                        "max_mem_level": 0.8,
                        "with_swap": false,
                        "max_pool_workers": 2,
                        "block_timeout": 60
                    }
                }
            ]
        },
        {
            "name": "NotificationQueueHandlerWorker",
            "kind": "QueueWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.content_parse",
                    "queue_name": "parse_notification",
                    "appsettings":{
                        "db_url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44_test",
                        "db_validate": true
                    }
                }
            ]
        }
    ]
}
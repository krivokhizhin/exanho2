{
    "exanho_context":{
        "db_connectings":[
            {
                "name": "eis44_test",
                "url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44_test",
                "models": [
                    "exanho.sample.model.__init__",
                    "exanho.ftp_loading.model.loading"
                ]
            }
        ],
        "joinable_queues": [
            {
                "name": "sample_parse",
                "maxsize": 1000
            },
            {
                "name": "parse_notification",
                "maxsize": 1000
            }
        ],
        "service_endpoints": [
            {
                "interface": "ISampleService",
                "host": "",
                "port": 3120
            },
            {
                "interface": "IKeyValueService",
                "host": "",
                "port": 3121
            }
        ]
    },
    "exanho_actors": [
        {
            "name": "SampleService",
            "kind": "XmlRpcServer",
            "daemon": true,
            "services":[
                {
                    "handler_module": "exanho.sample.services.sample",
                    "interface": "ISampleService",
                    "concurrency": {
                        "degree": 2,
                        "kind": "thread"
                    }
                },
                {
                    "handler_module": "exanho.sample.services.key_value",
                    "interface": "IKeyValueService",
                    "concurrency": {
                        "degree": 2,
                        "kind": "process"
                    },
                    "db_key": "eis44_test"
                }
            ]            
        },
        {
            "name": "NotificationQueueHandlerWorker",
            "kind": "QueueWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.content_parse",
                    "queue_name": "parse_notification",
                    "appsettings":{
                        "db_key": "eis44_test",
                        "db_validate": true
                    }
                }
            ]
        },
        {
            "name": "SampleTaskWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.sample.workers.sample",
                    "sleep": 2.0,
                    "appsettings":{
                        "joinable_queue": "sample_parse"
                    }
                }
            ]
        },
        {
            "name": "SampleQueueHandlerWorker",
            "kind": "QueueWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.sample.workers.queue_handler",
                    "queue_name": "sample_parse",
                    "appsettings":{
                        "db_key": "eis44_test",
                        "db_validate": true
                    }
                }
            ]
        }
    ]
}
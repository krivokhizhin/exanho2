{
    "exanho_actors": [
        {
            "name": "SampleService",
            "kind": "RpcServer",
            "daemon": true,
            "services":[
                {
                    "handler_module": "exanho.sample.services.sample",
                    "address": {
                        "host": "",
                        "port": 3120
                    },
                    "secret_key": "qwerty",
                    "concurrency": {
                        "degree": 2,
                        "kind": "thread"
                    }
                }
            ]            
        },
        {
            "name": "RunFtpTaskWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.task_run",
                    "sleep": 5.0,
                    "appsettings":{
                        "db_url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "db_validate": true,
                        "ftp_host": "ftp.zakupki.gov.ru",
                        "ftp_port": 21,
                        "ftp_user": "free",
                        "ftp_password": "free"

                    }
                }
            ]
        },
        {
            "name": "LoadFtpFileWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.file_load",
                    "sleep": 5.0,
                    "appsettings":{
                        "db_url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "db_validate": true,
                        "ftp_host": "ftp.zakupki.gov.ru",
                        "ftp_port": 21,
                        "ftp_user": "free",
                        "ftp_password": "free",
                        "max_mem_level": 0.8,
                        "with_swap": false,
                        "max_pool_workers": 20
                    }
                }
            ]
        },
        {
            "name": "ParseFtpContentWorker",
            "kind": "SleepWorker",
            "daemon": true,
            "workers": [
                {
                    "module": "exanho.ftp_loading.workers.content_parse",
                    "sleep": 5.0,
                    "concurrency": {
                        "degree": 1,
                        "kind": "process"
                    },
                    "appsettings":{
                        "db_url": "postgresql+psycopg2://kks:Nata1311@localhost/eis44",
                        "db_validate": true,
                        "rpc_port": 3121,
                        "rpc_secretkey": "pa$$word",
                        "max_pool_workers": 4,
                        "like_expression": ""
                    }
                }
            ]
        }
    ]
}
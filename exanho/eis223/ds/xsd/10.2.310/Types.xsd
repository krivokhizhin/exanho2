<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://zakupki.gov.ru/223fz/types/1" targetNamespace="http://zakupki.gov.ru/223fz/types/1" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:complexType name="documentUploadBaseData" abstract="true">
        <xs:annotation>
            <xs:documentation>Базовый тип для загружаемого документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="guid" type="guidType">
                <xs:annotation>
                    <xs:documentation>Идентификатор позиции в информационном пакете</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entityGuid" type="guidType">
                <xs:annotation>
                    <xs:documentation>Глобальный идентификатор сведений</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="placer" type="customerInfoType">
                <xs:annotation>
                    <xs:documentation>Данные организации, загружающей документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="additionalDocumentsCount" type="xs:positiveInteger">
                <xs:annotation>
                    <xs:documentation>Общее число документов, прикрепляемых дополнительно</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="documentNumber" type="xs:positiveInteger">
                <xs:annotation>
                    <xs:documentation>Номер текущего прикрепляемого документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="document" type="documentType">
                <xs:annotation>
                    <xs:documentation>Прикрепленный документ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="documentUpload">
        <xs:annotation>
            <xs:documentation>Загрузка дополнительного документа</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="orderClauseDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы положения о закупке</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
										<xs:element name="noticeDocumentUpload">
											<xs:annotation>
												<xs:documentation>Документы извещения о закупке</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="documentUploadBaseData">
														<xs:sequence>
															<xs:element name="isUploadComplete" type="xs:boolean"
																		default="false" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Признак «Загрузка завершена»
																	</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
                                        <xs:element name="protocolDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы протокола</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="purchaseContractDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы договора закупки</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="purchasePlanDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы плана закупок</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="purchasePlanProjectDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы проекта плана закупок</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="purchaseContractAccountDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы отчетности о договорах</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="purchaseRejectionDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы извещения об отказе</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="explanationDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы разъяснения</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="explanationRequestDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы запросов заказчика о разъяснении положений заявки</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="changeRequirementsDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы уточненных требований</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="contractDocumentUpload">
                                            <xs:annotation>
                                                <xs:documentation>Документы договора</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:complexContent>
                                                    <xs:extension base="documentUploadBaseData"/>
                                                </xs:complexContent>
                                            </xs:complexType>
                                        </xs:element>
										<xs:element name="contractCompletionDocumentUpload">
											<xs:annotation>
												<xs:documentation>Документы сведений об исполнении договора</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="documentUploadBaseData"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="contractCancellationDocumentUpload">
											<xs:annotation>
												<xs:documentation>Документы сведений о расторжении договора</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="documentUploadBaseData"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
                                    </xs:choice>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

	<xs:element name="confirmation">
		<xs:annotation>
			<xs:documentation>Протокол загрузки информационного пакета</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="packetType">
					<xs:sequence>
						<xs:element name="body">
							<xs:annotation>
								<xs:documentation>Данные информационного пакета</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="refguid" type="guidType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Идентификатор обработанного пакета</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="result" type="resultType">
										<xs:annotation>
											<xs:documentation>Результат обработки пакета.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="itemResults" type="itemResultListType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Результат обработки позиций</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="violations" type="violationListType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Выявленные в процессе обработки пакета нарушения и
                                                ошибки
                                            </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="refFileName" type="nonEmptyString">
										<xs:annotation>
											<xs:documentation>Имя обработанного файла</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="messageId" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Идентификатор сообщения для асинхронного взаимодействия</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="processingInfoRequest">
		<xs:annotation>
			<xs:documentation>Запрос статуса загрузки</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="packetType">
					<xs:sequence>
						<xs:element name="body">
							<xs:annotation>
								<xs:documentation>Данные информационного пакета</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="messages">
										<xs:annotation>
											<xs:documentation>Сообщения для асинхронного взаимодействия</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="message" maxOccurs="1000">
													<xs:annotation>
														<xs:documentation>Сообщение для асинхронного взаимодействия</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="messageId" type="xs:string">
																<xs:annotation>
																	<xs:documentation>Идентификатор сообщения для асинхронного взаимодействия</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="processingInfoResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос статуса загрузки</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="packetType">
					<xs:sequence>
						<xs:element name="body">
							<xs:annotation>
								<xs:documentation>Данные информационного пакета</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="refguid" type="guidType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Идентификатор обработанного пакета</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="result" type="resultType">
										<xs:annotation>
											<xs:documentation>Результат обработки пакета.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="violations" type="violationListType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Выявленные в процессе обработки пакета нарушения и ошибки</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="infos" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="info" minOccurs="1" maxOccurs="1000">
													<xs:annotation>
														<xs:documentation>Информаиця о загружаемых сведениях</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="guid" type="guidType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>GUID сведений</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="regNum" type="xs:string" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Реестровый номер сведений</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="placeDate" type="xs:dateTime" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Дата размещения</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="version" type="xs:int" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Номер версии</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="placer" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Информация об организации, разместившей сведения</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="name" type="xs:string" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>Наименование</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="inn" type="innType" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>ИНН</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="kpp" type="kppType" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>КПП</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="ogrn" type="ogrnType" minOccurs="0">
																			<xs:annotation>
																				<xs:documentation>ОГРН</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
															<xs:element name="status" type="statusType">
																<xs:annotation>
																	<xs:documentation>Статус обработки пакета.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="violations" type="violationListType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Выявленные в процессе загрузки сведений нарушения и ошибки</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="contactInfoType">
		<xs:annotation>
			<xs:documentation>Информация о контактном лице</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="firstName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Имя</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="nonEmptyString">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="middleName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Отчество</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="nonEmptyString">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lastName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фамилия</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="nonEmptyString">
						<xs:maxLength value="300"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="phone" type="phoneType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Телефон</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fax" type="phoneType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Факс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="email" type="emailType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Email</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="additionalContactInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дополнительная контактная информация</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cryptoSignsType">
		<xs:annotation>
			<xs:documentation>ЭЦП документа</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="signature" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>ЭЦП</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:base64Binary">
							<xs:attribute name="type" type="signatureType">
								<xs:annotation>
									<xs:documentation>Тип ЭЦП:
CAdES-BES;
CAdES-A</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="currencyType">
		<xs:sequence>
            <xs:choice>
                <xs:element name="letterCode">
                    <xs:annotation>
                        <xs:documentation>Код валюты</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="nonEmptyString">
                            <xs:maxLength value="3"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:sequence>
                    <xs:element name="code" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Код валюты</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="nonEmptyString">
                                <xs:maxLength value="3"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="digitalCode">
                        <xs:annotation>
                            <xs:documentation>Цифровой код валюты</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="nonEmptyString">
                                <xs:maxLength value="3"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:choice>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование валюты</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="nonEmptyString">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="customerInfoType">
		<xs:annotation>
			<xs:documentation>Данные заказчика</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mainInfo" type="customerMainInfoType">
				<xs:annotation>
					<xs:documentation>Основная информация об организации</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="supplierInfoType">
		<xs:annotation>
			<xs:documentation>Данные поставщика</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mainInfo" type="supplierMainInfoType">
				<xs:annotation>
					<xs:documentation>Основная информация об организации поставщика</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

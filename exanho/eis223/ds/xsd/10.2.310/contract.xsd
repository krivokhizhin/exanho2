<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://zakupki.gov.ru/223fz/contract/1"
           xmlns:t="http://zakupki.gov.ru/223fz/types/1" targetNamespace="http://zakupki.gov.ru/223fz/contract/1"
           elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:import namespace="http://zakupki.gov.ru/223fz/types/1" schemaLocation="Types.xsd"/>

    <xs:element name="contract">
        <xs:annotation>
            <xs:documentation>Договор</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="t:packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="item" type="contractItemType">
                                        <xs:annotation>
                                            <xs:documentation>Позиция в информационном пакете</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="performanceContract">
        <xs:annotation>
            <xs:documentation>Сведения об исполнении договора</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="t:packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="item" type="performanceContractItemType">
                                        <xs:annotation>
                                            <xs:documentation>Позиция в информационном пакете</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="contractCancellation">
        <xs:annotation>
            <xs:documentation>Сведения об расторжении договора</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="t:packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="item" type="contractCancellationItemType">
                                        <xs:annotation>
                                            <xs:documentation>Позиция в информационном пакете</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="subcontractorInfo">
        <xs:annotation>
            <xs:documentation>Сведений о договоре с субподрядчиком</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="t:packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="item" type="subcontractorInfoItemType">
                                        <xs:annotation>
                                            <xs:documentation>Позиция в информационном пакете</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="electronicContract">
        <xs:annotation>
            <xs:documentation>Сведения о договоре в электронной форме</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="t:packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="item" type="electronicContractItemType">
                                        <xs:annotation>
                                            <xs:documentation>Позиция в информационном пакете</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="contractItemType">
        <xs:annotation>
            <xs:documentation>Данные позиции в пакете</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="t:itemType">
                <xs:sequence>
                    <xs:element name="contractData" type="contractDataType">
                        <xs:annotation>
                            <xs:documentation>Сведения о договоре</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="contractCancellationItemType">
        <xs:annotation>
            <xs:documentation>Данные позиции в пакете</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="t:itemType">
                <xs:sequence>
                    <xs:element name="contractCancellationData" type="contractCancellationDataType">
                        <xs:annotation>
                            <xs:documentation>Сведения об расторжении договора</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="performanceContractItemType">
        <xs:annotation>
            <xs:documentation>Данные позиции в пакете</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="t:itemType">
                <xs:sequence>
                    <xs:element name="performanceContractData" type="performanceContractDataType">
                        <xs:annotation>
                            <xs:documentation>Сведения об исполнении договора</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="subcontractorInfoItemType">
        <xs:annotation>
            <xs:documentation>Данные позиции в пакете</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="t:itemType">
                <xs:sequence>
                    <xs:element name="subcontractorInfoData" type="subcontractorInfoDataType">
                        <xs:annotation>
                            <xs:documentation>Сведений о договоре с субподрядчиком</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="electronicContractItemType">
        <xs:annotation>
            <xs:documentation>Данные позиции в пакете</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="t:itemType">
                <xs:sequence>
                    <xs:element name="electronicContractInfoData" type="electronicContractDataType">
                        <xs:annotation>
                            <xs:documentation>Сведений о договоре в электронной форме</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="contractDataType">
        <xs:annotation>
            <xs:documentation>Сведения о договоре</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="guid" type="t:guidType">
                <xs:annotation>
                    <xs:documentation>Глобальный идентификатор сведений о договоре</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registrationNumber" type="t:registrationNumber23Type" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Регистрационный номер сведений о договоре</xs:documentation>
                </xs:annotation>
            </xs:element>

            <!--BEGIN FZNF-564 -->
            <xs:element name="notice44" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак извещение по 44-ФЗ</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="noticeNotPlacedByFz223p5s4" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак "Извещение не размещалось в соответствии с частью 5 статьи 4 Федерального Закона № 223-ФЗ"</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="notice44Num" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер извещения о закупке по 44-ФЗ</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="200"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="lot44Num" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер лота по 44-ФЗ</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="200"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="termination" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак «Договором предусмотрено прекращение обязательств сторон по договору в связи с окончанием срока действия договора»</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="extension" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак «Договором предусмотрена возможность продления срока действия договора после его окончания»</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="prolongation" type="xs:boolean" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак «Пролонгация»</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="customerAppealedOrNeedsApproval" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак «Действия (бездействия) заказчика обжалованы в антимонопольном органе или существует необходимость одобрения органом управления заказчика в соответствии с законодательством Российской Федерации заключения договора»</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="customerApprovalOrAntimonopolyDescisionDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата одобрения органом управления заказчика или вынесения решения антимонопольного органа</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="startExecutionTerm" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Условие начала исполнения договора</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="endExecutionTerm" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Условие начала исполнения договора</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="lotNum" minOccurs="0" type="xs:long">
                <xs:annotation>
                    <xs:documentation>Номер лота</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="planPosition" type="t:planInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения о позиции плана закупки (признак 44-ФЗ или признак "Извещение не размещалось в соответствии с частью 5 статьи 4 Федерального Закона № 223-ФЗ" должен быть задан)</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="urlEIS" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Url документа в ЕИС</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="3000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="urlVSRZ" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Url документа в ВСРЗ</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="3000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="urlKisRmis" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Url документа в КИС/РМИС</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="3000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="createDateTime" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата создания сведений о договоре</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="customer" type="t:customerInfoType">
                <xs:annotation>
                    <xs:documentation>Данные заказчика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="placer" type="t:customerInfoType">
                <xs:annotation>
                    <xs:documentation>Данные организации, размещающей сведения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="detachedOrg" type="t:customerInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Данные обособленного подразделения заказчика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="publicationDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата размещения сведений о договоре</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="contractStatusType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Допустимые значения (F - Редактирование, P  - Размещено, I - Недействительно, M  Изменение)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="version" type="t:versionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Версия сведений</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="modificationDescription" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Причина изменения информации о договоре</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="digitalPurchase" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак закупки в электронном виде</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="digitalPurchaseCode">
                <xs:annotation>
                    <xs:documentation>Код признака закупки в электронном виде</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="6"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="emergency" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак "Закупка осуществляется вследствие аварии, иных чрезвычайных ситуаций природного или техногенного характера, непреодолимой силы, при необходимости срочного медицинского вмешательства, а также для предотвращения угрозы возникновения указанных ситуаций"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="provider" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак "Закупка у субъекта малого и среднего предпринимательства, в том числе закупка, участниками которой являются только субъекты малого и среднего предпринимательства"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="providerCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код признака "Закупка у субъекта малого и среднего предпринимательства, в том числе закупка, участниками которой являются только субъекты малого и среднего предпринимательства" (30 если provider = true)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="changeContract" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Признак "Изменение договора"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="attachments" type="t:documentListType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Прикрепленные документы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contractRegNumber" type="t:registrationNumber23Type" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Регистрационный номер договора</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование сведений о договоре</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="500"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="contractDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата заключения договора</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="approveDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата утверждения изменения сведений о договоре</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="purchaseNoticeInfo" type="purchaseNoticeInfoType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Информация о закупке</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="lotGuid" type="t:guidType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Глобальный идентификатор лота</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="subjectContract">
                <xs:annotation>
                    <xs:documentation>Предмет договора</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="purchaseTypeInfo">
                <xs:annotation>
                    <xs:documentation>Информация о способе закупки (11011 - Конкурс, 12012 - Аукцион, 30000 - Закупка у единственного поставщика/подрядчика/исполнителя, 40000 - Иной способ закупки, предусмотренный правовым актом заказчика, указанным в части 1 статьи 2 Федерального закона)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="code">
                            <xs:annotation>
                                <xs:documentation>Код способа закупки</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="t:nonEmptyString">
                                    <xs:maxLength value="5"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="name" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Название способа закупки</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="t:nonEmptyString">
                                    <xs:maxLength value="500"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="resumeDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата подведения итогов закупки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="supplierInfo" type="supplierMainType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Информация о сведениях поставщика</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="hasSubcontractor" type="xs:boolean" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Признак "К исполнению договора должны быть привлечены субподрядчики (соисполнители) из числа субъектов малого и (или) среднего предпринимательства"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="hasSubcontractorCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код признака "К исполнению договора должны быть привлечены субподрядчики (соисполнители) из числа субъектов малого и (или) среднего предпринимательства"</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="subcontractorsTotal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Общая стоимость договоров, которые будут заключены поставщиком (подрядчиком, исполнителем) с субподрядчиками (соисполнителями) из числа субъектов малого и среднего предпринимательства для целей исполнения договора с заказчиком</xs:documentation>
                </xs:annotation>
                <xs:simpleType>

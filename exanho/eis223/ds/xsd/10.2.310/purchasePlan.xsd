                    <xs:documentation>Реестровый номер контракта 44-ФЗ</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:pattern value="\d{19}"/>
                    </xs:restriction>

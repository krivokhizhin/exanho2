            </xs:element>
            <xs:element name="contractEndStageDate" minOccurs="0" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата окончания этапа контракта 44-ФЗ</xs:documentation>
                </xs:annotation>

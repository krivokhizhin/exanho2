									<xs:documentation>Срок, на который предоставляется гарантия</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:sequence>
											<xs:annotation>
												<xs:documentation>Срок в формате интервала дат</xs:documentation>
											</xs:annotation>
											<xs:element name="fromDate" type="xs:date">

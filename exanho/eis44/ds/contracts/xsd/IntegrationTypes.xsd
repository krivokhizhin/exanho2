									<xs:enumeration value="FINAL"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="autoExDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата автоматического исключения</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
